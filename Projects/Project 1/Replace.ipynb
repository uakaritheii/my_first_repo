   "source": [
    "with open('edit.txt', 'r') as f:\n",
    "    txt = f.readlines()[0]\n",
    "\n",
    "newlst = []\n",
    "\n",
    "for w in txt.split(' '):\n",
    "    if len(w) == 4:\n",
    "        wrd = f\"{w[0]}***\"\n",
    "    else:\n",
    "        wrd = w\n",
    "    newlst.append(wrd)\n",
    "\n",
    "text = ' '.join(newlst)\n",
    "\n",
    "with open('clean.txt', 'w') as f:\n",
    "    f.write(text)"
   ]
  }
